{
    "semanticCommits": true,
    "packageRules": [
    {
        "groupName": "Maven Test scope",
        "groupSlug": "testscope",
        "matchManagers": ["maven"],
        "matchDepTypes": ["test"]
  
    },
    {
        "groupName": "High Confidence",
        "matchPackagePatterns": [
          "^org.webjars:",
          "^org.eclipse.jetty:",
          "^org.asciidocter:"
        ]
    },
    {
      "groupName": "Maven Plugins",
      "matchPackagePatterns": [
        "^org.apache.maven.plugins:"
      ]
  }

    ]
}
